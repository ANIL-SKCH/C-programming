
//C program to Find the Largest Number Among Three Numbers.
#include <stdio.h>
int main(){
    int a,b,c;
    printf("Enter the numbers :");
    scanf("%d %d %d",&a,&b,&c);
    if(a>=b && a>=c){
        printf("%d is the largest number",a);
    }
    if(b>=a && b>=c){
        printf("%d is the largest number",b);
    }
    else{
        printf("%d is the largest number",c);
    }
}
