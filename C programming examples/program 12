// To the given number is beon number or not.
#include <stdio.h>
int main() {
  int a,b,sq,d,sum=0;
  printf("Enter the number : ");
  scanf("%d",&a);
  sq=a*a;
  printf("the square of the given number is : %d \n",sq);
  b=sq;
  while(b!=0){
      d=b%10;
      sum=sum+d;
      b=b/10;
  }
  printf("the sum of the square numbers is : %d\n",sum);
  if(a==sum){
      printf("%d is a neon number.",a);
  } else {
      printf("%d is not a neon number.",a);
  }
    return 0;
}
