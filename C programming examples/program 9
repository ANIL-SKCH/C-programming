// To check number is a prime or not prime.
#include <stdio.h>
int main() {
    int n, i, f = 1;;
    printf("Enter the number: ");
    scanf("%d",&n);

    for (i = 2; i < n ; i++) {
        if (n % i == 0) {
            f = 0; 
            break;
        }
    }

    if (f==1) {
        printf("%d is a prime number.\n", n);
    } else {
        printf("%d is not a prime number.\n", n);
    }

    return 0;
}
