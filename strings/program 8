//Write a C program to combine two string in a single String.
#include<stdio.h>
#include <string.h>
int main() {
 char s[50],t[50],u[50];
 int i,j,n1,n2,n3;
 printf("enter the string1 : ");
 gets(s);
 printf("enter the string2 : ");
 gets(t);
 n1=strlen(s);
 n2=strlen(t);
 n3=n2+n1;
 int n=0;
 for(i=0;i<n1;i++){
    u[n++]=s[i];
 }
 for(j=0;j<n2;j++){
    u[n++]=t[j];
 }
 printf("the resulting string are :");
 for(i=0;i<n3;i++){
    printf("%c",u[i]);
}
return 0;
}
