// Write a C program to remove all the Spaces from a given String.
#include<stdio.h>
#include<string.h>
int main() {
char s[50],ch;
int i,n,j=0;
printf("enter the string : ");
gets(s);
printf("Enter the character you want to remove : ");
scanf("%c",&ch);
n=strlen(s);
for(i=0;i<n;i++){
    if(s[i]!=32){
        s[j++]=s[i];
        
    }
  
}
  s[j]='\0';
  printf("the resulting string is %c: %s\n",ch,s);

return 0;
}
