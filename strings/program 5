//Write a c program to print all the frequency of character present.
#include<stdio.h>
int main()
{
    char s[50],ch;
    int c=0,i;
    printf("enter the string : ");
    gets(s);
    for(ch='a';ch<'z';ch++){
       c=0;
        for(int i=0;s[i]!='\0';i++){
            if(ch==s[i]){
                c++;
  
            }
        }
            if(c>0){
                printf("the frequency of %c is %d\n",ch,c);
            }
        }
    
    return 0;
}
