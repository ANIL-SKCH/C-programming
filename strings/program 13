// Create a program that compresses a string by replacing repeated characters with a count of the repetition 
#include<stdio.h>
int main()
{
    char s[50],ch;
    int c=0;
    printf("enter the string : ");
    gets(s);
    for(ch='a';ch<'z';ch++){
       c=0;
        for(int i=0;s[i]!='\0';i++){
            if(ch==s[i]){
                c++;
                
            }
        }
            if(c!=0){
                if(c>=2){
                printf("%c%d",ch,c);
                }
            }
        }
    
    return 0;
}
